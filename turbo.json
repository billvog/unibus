{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "SENTRY_ORG",
        "SENTRY_PROJECT",
        "SENTRY_AUTH_TOKEN"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false,
      "env": [
        "NODE_ENV",
        "SKIP_ENV_VALIDATION"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ],
      "cache": false
    },
    "lingui:extract": {
      "dependsOn": [
        "^lingui:extract"
      ],
      "outputs": [
        "src/locales/*.po"
      ]
    }
  },
  "globalEnv": [
    "NEXT_RUNTIME"
  ]
}